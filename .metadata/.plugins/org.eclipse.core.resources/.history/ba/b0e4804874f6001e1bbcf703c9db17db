package exemplos;

public class Programa06 {
	
	public static void main(String [] args) {
		
		//escopo de variÃ¡vel 
		int numero =10;
		if(numero > 0) {
		int segundoNum = 15;
		int soma = numero + segundoNum;
		System.out.println("A soma deu " + soma);
		}
		System.out.println("A soma deu " + soma);
		
	}

}
